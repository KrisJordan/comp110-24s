{
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.autoImportCompletions": true,
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.vscode": true,
        "**/.devcontainer": true,
        "**/.flake8": true,
        "**/.gitignore": true,
        "**/pyrightconfig.json": true,
        "**/requirements.txt": true,
    }
}